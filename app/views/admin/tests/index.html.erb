<h1>Все тесты</h1>

<p>
  <%= link_to 'Новый тест', new_admin_test_path %><br>
  <%= link_to 'Gists', admin_gists_path %><br>
  <%= link_to 'Badges', admin_badges_path %>
</p>

<%= render 'shared/errors', resource: @test %>

<table class="table">
  <tr>
    <th class="sort-by-title pointer">
      Title
      <%= octicon 'arrow-up', class: 'text-success hide' %>
      <%= octicon 'arrow-down', class: 'text-success hide' %>
    </th>
    <th>Level</th>
    <th>Number of questions</th>
    <th colspan="4">Действия</th>
  </tr>

  <%= render @tests %>
</table>
